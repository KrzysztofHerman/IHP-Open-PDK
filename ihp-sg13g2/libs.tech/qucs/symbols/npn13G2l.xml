<?xml version="1.0"?>
<Component
  xsi:noNamespaceSchemaLocation="Component.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  library="IHP 130 BiCMOS components" names="Q" schematic_id = "npn13G2l">
    <Description>
        SG13G2 HBT transistor with configurable emmiter length
    </Description>
    <Models>
        <DefaultModel value="npn13G2l">
        </DefaultModel>
        <SpiceModel value="npn13G2l">
        </SpiceModel>
    </Models>
    <Netlists>
        <NgspiceNetlist value = 'XQ{PartCounter} {nets} npn13G2l Nx={Nx} El={El}'>
        </NgspiceNetlist>
        <CDLNetlist value = 'Q{PartCounter} {nets} npn13G2l Nx={Nx}'> </CDLNetlist>
    </Netlists>
    <Symbols>
        <Symbol id="Standard">
           <File>{QUCS_S_COMPONENTS_LIBRARY}/bjt.sym</File>
        </Symbol>
    </Symbols>
    <Parameters>
        <Parameter name="Nx" unit="" default_value="1" show="true">
            <Description>Number of emitters</Description>
        </Parameter>
        <Parameter name="El" unit="" default_value="2.5" show="true">
            <Description>Emitter length</Description>
        </Parameter>
    </Parameters>
</Component>
